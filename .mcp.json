{
  "mcpServers": {
    "next-devtools": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "next-devtools-mcp@latest"
      ]
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp?project_ref=hzowvejjmnamhnrbqpou"
    },
    "shadcn": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "shadcn@latest",
        "mcp"
      ]
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@executeautomation/playwright-mcp-server"]
    }
  }
}
